{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "preserve",
    "paths": {
      "@spartan-ng/helm/sidebar": [
        "./src/app/core/layout/sidebar/src/index.ts"
      ],
      "@spartan-ng/helm/utils": [
        "./src/app/core/layout/utils/src/index.ts"
      ],
      "@spartan-ng/helm/button": [
        "./src/app/core/layout/button/src/index.ts"
      ],
      "@spartan-ng/helm/icon": [
        "./src/app/core/layout/icon/src/index.ts"
      ],
      "@spartan-ng/helm/input": [
        "./src/app/core/layout/input/src/index.ts"
      ],
      "@spartan-ng/helm/separator": [
        "./src/app/core/layout/separator/src/index.ts"
      ],
      "@spartan-ng/helm/sheet": [
        "./src/app/core/layout/sheet/src/index.ts"
      ],
      "@spartan-ng/helm/tooltip": [
        "./src/app/core/layout/tooltip/src/index.ts"
      ],
      "@spartan-ng/helm/skeleton": [
        "./src/app/core/layout/skeleton/src/index.ts"
      ],
      "@spartan-ng/helm/collapsible": [
        "./src/app/core/layout/collapsible/src/index.ts"
      ],
      "@spartan-ng/helm/avatar": [
        "./src/app/core/layout/avatar/src/index.ts"
      ],
      "@spartan-ng/avatar/avatar": [
        "./src/app/core/layout/avatar/src/index.ts"
      ],
      "@spartan-ng/avatar/utils": [
        "./src/app/core/layout/utils/src/index.ts"
      ],
      "@spartan-ng/avatar/dropdown-menu": [
        "./frontend/src/app/core/layout/dropdown-menu/src/index.ts"
      ],
      "@spartan-ng/avatar/icon": [
        "./frontend/src/app/core/layout/icon/src/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "typeCheckHostBindings": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}
